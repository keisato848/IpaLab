{
  "name": "ipa-lab",
  "version": "0.0.0",
  "packageManager": "npm@10.9.2",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "postbuild": "npm run sbom",
    "sbom": "cyclonedx-bom --output-format json --output-file sbom.json",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "engines": {
    "node": "20"
  },
  "devDependencies": {
    "turbo": "^2.7.2",
    "prettier": "^3.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}